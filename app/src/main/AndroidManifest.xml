<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.miwok">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Phrases"
            android:label="@string/phrases_activity"></activity>

        <activity android:name=".Colors"
            android:label="@string/colors_activity"/>

        <activity android:name=".FamilyMembers"
            android:label="@string/family_activity"/>

        <activity android:name=".NumbersActivity"
            android:label="@string/number_activity"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>




</manifest>